{
	"schemaVersion": 1,
	"id": "no-more-ravines",
	"version": "${version}",
	"name": "No more ravines",
	"description": "A minecraft fabric mod that disables natural ravine generation in the overworld.",
	"authors": [
		"Neluxx"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/no-more-ravines/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"neluxx.no_more_ravines.NoMoreRavines"
		],
		"client": [
			"neluxx.no_more_ravines.NoMoreRavinesClient"
		]
	},
	"mixins": [
		"no-more-ravines.mixins.json",
		{
			"config": "no-more-ravines.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.2",
		"minecraft": ">=1.19 <=1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
